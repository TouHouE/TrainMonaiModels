{
  "model": {
    "name": "unet",
    "param": {
      "spatial_dims": 3,
      "in_channels": 1,
      "out_channels": 1,
      "channels": [16, 32, 64, 128, 256],
      "strides": [2, 2, 2, 2],
      "num_res_units": 2
    }
  },
  "data": {
    "image_root": "/mnt/src/data",
    "json_path": "/mnt/src/data/vista_table.json",
    "fold": 1
  },
  "loader": {
    "train": {
      "batch_size": 1,
      "shuffle": true,
      "pin_memory": true,
      "num_workers": 2
    },
    "val": {
      "batch_size": 1,
      "shuffle": false,
      "pin_memory": true,
      "num_workers": 0
    },
    "test": {
      "batch_size": 1,
      "shuffle": false,
      "pin_memory": true,
      "num_workers": 0
    }
  },
  "optim": {
    "name": "adamw",
    "param": {
	    "lr": 1e-4
    }
  },
  "logs": {
    "logger": ["wandb", "tqdm"],
    "name": "sup-unet-3d",
    "exp_root": "/mnt/workspace/project/TrainMonaiModels/ckpt",
    "project": "Sup-3dUNet"
  }

}
